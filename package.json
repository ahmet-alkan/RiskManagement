  "dependencies": {
    "@sap-cloud-sdk/http-client": "2.15.0",
    "@sap-cloud-sdk/resilience": "^3.7.0",
    "@sap/cds": "^7.3.1",
    "@sap/cds-hana": "^2",
    "@sap/cds-mtxs": "^1.12.1",
    "@sap/xssec": "^3",
    "csj": "^1.0.0",
    "express": "^4",
    "passport": "^0",
    "resilience": "^1.0.2"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^7",
    "@sap/ux-specification": "^1.120.0"
